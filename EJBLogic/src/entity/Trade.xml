<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="entity.Trade">
        <table name="trade" schema="otc" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="buyOrderId">
                <column name="buyOrderId" nullable="false" precision="11"/>
            </basic>
            <basic name="sellOrderId">
                <column name="sellOrderId" nullable="false" precision="11"/>
            </basic>
            <basic name="time">
                <column name="time" length="45"/>
            </basic>
            <basic name="quantity">
                <column name="quantity" nullable="false" precision="11"/>
            </basic>
            <basic name="productId">
                <column name="productId" nullable="false" precision="11"/>
            </basic>
            <basic name="buyerId">
                <column name="buyerId" nullable="false" precision="11"/>
            </basic>
            <basic name="sellerId">
                <column name="sellerId" nullable="false" precision="11"/>
            </basic>
            <basic name="brokerId">
                <column name="brokerId" nullable="false" precision="11"/>
            </basic>
            <basic name="price">
                <column name="price" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
