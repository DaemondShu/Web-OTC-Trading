<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="entity.Trade">
        <table name="trade" schema="otc" />
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="time">
                <column name="time" length="45"/>
            </basic>
            <basic name="quantity">
                <column name="quantity" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
